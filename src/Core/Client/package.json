{
  "name": "web-application-demo",
  "version": "1.0.0",
  "description": "React, material, componnts etc.",
  "scripts": {
    "dev": "webpack && webpack-dev-server --progress --watch --colors --inline",
    "start": "webpack-dev-server --progress --colors --hot --config ./webpack.config.js",
    "clean": "rm -rf www",
    "build": "npm run clean && mkdir www && babel server -s -d www",
    "production": "npm run build && node bin/production",
    "start1s": "electron main.js",
    "start2": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "lint": "eslint Application",
    "test": "mocha $(find Application -name '*.spec.js') --compilers js:babel-register --require ./Tests/Helpers.js --require ./Tests/Dom.js --recursive",
    "test:watch": "npm test -- --watch",
    "start3": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist"
  },
  "keywords": [
    "react",
    "tutorial"
  ],
  "author": "Achal",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "material-design-lite": "^1.3.0",
    "normalizr": "^3.2.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^2.0.0-pre.6",
    "webpack-dev-server": "^2.3.0"
  }
}
